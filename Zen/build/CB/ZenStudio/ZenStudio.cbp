<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ZenStudio" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Script file="lib_finder.script" />
			<Target title="Debug-GCC">
				<Option output="$(#ZEN.lib)/ZenStudio_d" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-fPIC" />
					<Add option="`wx-config --cflags`" />
					<Add option="-D__WXGTK__" />
				</Compiler>
				<Linker>
					<Add option="`wx-config --libs`" />
					<Add option="`pkg-config --libs boost`" />
					<Add library="pthread" />
					<Add library="Core_d" />
					<Add library="Workbench_d" />
					<Add library="Enterprise_d" />
					<Add library="WorkbenchCommon_d" />
					<Add library="Engine_d" />
				</Linker>
			</Target>
			<Target title="Release-GCC">
				<Option output="$(#ZEN.lib)/ZenStudio" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-fPIC" />
					<Add option="-D__WXGTK__" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`wx-config --libs`" />
					<Add library="pthread" />
				</Linker>
			</Target>
			<Target title="Debug-MSVC5">
				<Option output="$(#ZEN.lib)/Core_d" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="msvc8" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/arch:SSE2" />
					<Add option="/Gd" />
					<Add option="/RTCu" />
					<Add option="/EHc" />
					<Add option="/EHs" />
					<Add option="/GR" />
					<Add option="/Od" />
					<Add option="/W3" />
					<Add option="/DTHREADMODEL_WIN32" />
					<Add option="/DHOST_WIN32" />
					<Add directory="$(#WindowsPlatformSDK.include)" />
				</Compiler>
				<Linker>
					<Add library="libxml2" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DZENCORE_EXPORTS" />
			<Add directory="$(#ZEN.include)" />
			<Add directory="$(#BOOST.include)" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.setup)" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add directory="$(#BOOST.lib)" />
			<Add directory="$(#ZEN.lib)" />
			<Add directory="$(#WX.lib)" />
			<Add directory="/home/sgtflame/dev/lib" />
		</Linker>
		<Unit filename="../../../Studio/ZenStudio/WorkbenchLocationDlg.cpp" />
		<Unit filename="../../../Studio/ZenStudio/WorkbenchLocationDlg.h" />
		<Unit filename="../../../Studio/ZenStudio/ZenStudio.cpp" />
		<Unit filename="../../../Studio/ZenStudio/ZenStudio.hpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1">
				<lib name="libxml-2.0" />
				<lib name="boost" />
			</lib_finder>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
