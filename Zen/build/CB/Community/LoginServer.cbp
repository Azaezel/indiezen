<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="LoginServer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Script file="lib_finder.script" />
			<Target title="Debug-GCC">
				<Option output="$(#ZEN.lib)\LoginServer_d" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-fPIC" />
				</Compiler>
				<Linker>
					<Add library="pthread" />
				</Linker>
			</Target>
			<Target title="Release-GCC">
				<Option output="$(#ZEN.lib)\LoginServer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-fPIC" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="pthread" />
				</Linker>
			</Target>
			<Target title="Debug-MSVC5">
				<Option output="$(#ZEN.lib)\LoginServer_d" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="msvc8" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/arch:SSE2" />
					<Add option="/Gd" />
					<Add option="/RTCu" />
					<Add option="/EHc" />
					<Add option="/EHs" />
					<Add option="/GR" />
					<Add option="/Od" />
					<Add option="/W3" />
					<Add option="/DTHREADMODEL_WIN32" />
					<Add option="/DHOST_WIN32" />
					<Add directory="$(#WindowsPlatformSDK.include)" />
				</Compiler>
				<Linker>
					<Add library="libxml2" />
					<Add library="Core_d.lib" />
					<Add library="Enterprise_d.lib" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DLOGINSERVER_EXPORTS" />
			<Add directory="$(#ZEN.include)" />
			<Add directory="$(#BOOST.include)" />
			<Add directory="$(#LIBXML2.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(#BOOST.lib)" />
			<Add directory="$(#ZEN.lib)" />
			<Add directory="$(#LIBXML2.lib)" />
		</Linker>
		<Unit filename="..\..\..\Community\LoginServer\Configuration.hpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\LoginServerPlugin.cpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\LoginService.cpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\LoginService.hpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\LoginServiceFactory.cpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\LoginServiceFactory.hpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\Session.cpp" />
		<Unit filename="..\..\..\Community\LoginServer\src\Session.hpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1">
				<lib name="libxml-2.0" />
				<lib name="boost" />
			</lib_finder>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
