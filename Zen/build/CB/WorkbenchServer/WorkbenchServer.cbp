<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="WorkbenchServer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Script file="lib_finder.script" />
			<Target title="Debug-GCC">
				<Option output="$(#ZEN.lib)/libWorkbenchServer_d" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-fPIC" />
					<Add option="-D__WXGTK__" />
				</Compiler>
				<Linker>
					<Add library="pthread" />
				</Linker>
			</Target>
			<Target title="Release-GCC">
				<Option output="$(#ZEN.lib)/libWorkbenchServer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-fPIC" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="pthread" />
				</Linker>
			</Target>
			<Target title="Debug-MSVC5">
				<Option output="$(#ZEN.lib)/WorkbenchServer_d" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="msvc8" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/arch:SSE2" />
					<Add option="/Gd" />
					<Add option="/RTCu" />
					<Add option="/EHc" />
					<Add option="/EHs" />
					<Add option="/GR" />
					<Add option="/Od" />
					<Add option="/W3" />
					<Add option="/DTHREADMODEL_WIN32" />
					<Add option="/DHOST_WIN32" />
					<Add directory="$(#WindowsPlatformSDK.include)" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DZENCORE_EXPORTS" />
			<Add directory="$(#ZEN.include)" />
			<Add directory="$(#BOOST.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(#BOOST.lib)" />
			<Add directory="$(#ZEN.lib)" />
		</Linker>
		<Unit filename="../../../Studio/WorkbenchServer/plugin.xml" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ColumnHeader.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ColumnHeader.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerController.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerController.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerModel.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerModel.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerNode.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/ProjectExplorerNode.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SceneController.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SceneController.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SceneModel.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SceneModel.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SpreadSheetController.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SpreadSheetController.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SpreadSheetDocument.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/SpreadSheetDocument.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/WorkbenchServerPlugin.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/WorkbenchService.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/WorkbenchService.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/WorkbenchServiceFactory.cpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/WorkbenchServiceFactory.hpp" />
		<Unit filename="../../../Studio/WorkbenchServer/src/subscription.hpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1">
				<lib name="boost" />
			</lib_finder>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
